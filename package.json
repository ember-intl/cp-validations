{
  "name": "@ember-intl/cp-validations",
  "version": "7.0.0",
  "description": "ember-intl support for ember-cp-validations",
  "keywords": [
    "ember-addon",
    "ember-cp-validations",
    "ember-intl",
    "ember-validation",
    "i18n",
    "internationalization"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ember-intl/cp-validations.git"
  },
  "license": "MIT",
  "author": "jason.mitchell.w@gmail.com",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "release:changelog": "changeset version",
    "release:package": "changeset publish",
    "start": "ember serve",
    "test": "ember test --test-port=0",
    "test:ember-compatibility": "./node_modules/.bin/ember try:one"
  },
  "dependencies": {
    "@babel/core": "^7.24.7",
    "ember-auto-import": "^2.7.4",
    "ember-cli-babel": "^8.2.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.24.7",
    "@babel/plugin-proposal-decorators": "^7.24.7",
    "@changesets/cli": "^2.27.7",
    "@changesets/get-github-info": "^0.6.0",
    "@ember/optional-features": "^2.1.0",
    "@ember/test-helpers": "^3.3.0",
    "@embroider/test-setup": "^3.0.3",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^8.2.2",
    "ember-cli": "~5.10.0",
    "ember-cli-dependency-checker": "^3.3.2",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-cp-validations": "^6.0.1",
    "ember-intl": "^7.0.4",
    "ember-load-initializers": "^2.1.2",
    "ember-page-title": "^8.2.3",
    "ember-qunit": "^8.0.2",
    "ember-resolver": "^11.0.1",
    "ember-source": "~5.10.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^5.13.0",
    "ember-try": "^3.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-ember": "^12.1.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^17.9.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-qunit": "^8.1.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "latest-version": "^9.0.0",
    "loader.js": "^4.7.0",
    "prettier": "^3.3.2",
    "qunit": "^2.21.0",
    "qunit-dom": "^3.2.0",
    "webpack": "^5.92.1"
  },
  "peerDependencies": {
    "ember-cp-validations": "^6.0.0",
    "ember-intl": "^6.1.0 || ^7.0.0"
  },
  "peerDependenciesMeta": {
    "ember-intl": {
      "optional": true
    }
  },
  "engines": {
    "node": "18.* || >= 20"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "after": [
      "ember-cp-validations"
    ]
  }
}
