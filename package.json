{
  "name": "@ember-intl/cp-validations",
  "version": "6.0.0",
  "description": "ember-intl support for ember-cp-validations",
  "keywords": [
    "ember-addon",
    "ember-cp-validations",
    "ember-intl",
    "ember-validation",
    "i18n",
    "internationalization"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ember-intl/cp-validations.git"
  },
  "license": "MIT",
  "author": "jason.mitchell.w@gmail.com",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "release:changelog": "changeset version",
    "release:package": "changeset publish",
    "start": "ember serve",
    "test": "ember test --test-port=0",
    "test:ember-compatibility": "./node_modules/.bin/ember try:one"
  },
  "dependencies": {
    "ember-cli-babel": "^7.26.11"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.7",
    "@changesets/get-github-info": "^0.6.0",
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.8.1",
    "@embroider/test-setup": "^1.8.3",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^8.2.2",
    "ember-auto-import": "^2.4.3",
    "ember-cli": "~4.8.0",
    "ember-cli-dependency-checker": "^3.3.1",
    "ember-cli-htmlbars": "^6.1.1",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-cp-validations": "^4.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-intl": "^5.7.2",
    "ember-load-initializers": "^2.1.2",
    "ember-page-title": "^7.0.0",
    "ember-qunit": "^6.0.0",
    "ember-resolver": "^8.0.3",
    "ember-source": "~4.8.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^4.16.1",
    "ember-try": "^2.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-ember": "^11.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-qunit": "^7.3.1",
    "latest-version": "^9.0.0",
    "loader.js": "^4.7.0",
    "prettier": "^2.7.1",
    "qunit": "^2.19.2",
    "qunit-dom": "^2.0.0",
    "webpack": "^5.74.0"
  },
  "engines": {
    "node": "18.* || >= 20"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "after": [
      "ember-cp-validations"
    ]
  }
}
